{
    "argv": [
        "python",
        "-m",
        "ipykernel_launcher",
        "-f",
        "{connection_file}"
    ],
    "display_name": "Python 3 (ipykernel)",
    "language": "python",
    "metadata": {
        "debugger": true
    },
    "env": {
        "PATH": "/usr/lib/jvm/java-8-openjdk-amd64/jre/bin/:${PATH}",
        "JAVA_HOME": "/usr/lib/jvm/java-8-openjdk-amd64"
    }
}