{
  "compilerOptions": {
    "target": "ES2019",
    "downlevelIteration": true,
    "module": "esnext",
    "jsx": "react",
    "strict": true,
    "moduleResolution": "node",
    "composite": true,
    "declarationMap": true,
    "baseUrl": "./",
    "rootDir": "./",
    "outDir": "tmp/ts",
    "allowSyntheticDefaultImports": true,
    "lib": [
      "esnext",
      "dom",
      "dom.iterable"
    ],
    "types": [],
    "noEmit": true,
    "resolveJsonModule": true
  },
  "include": [
    "src/**/*",
  ]
}